{
  "expo": {
    "name": "Instagram",
    "slug": "instagram",
    "scheme": "instagram",
    "icon": "./src/assets/icons/insta-logo-rounded.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./src/assets/icons/insta-logo-rounded.png",
        "resizeMode": "cover",
        "backgroundColor": "#0a0a0a",
        "dark": {
          "image": "./src/assets/icons/insta-logo-rounded.png",
          "resizeMode": "cover",
          "backgroundColor": "#0a0a0a"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/insta-logo-rounded.png",
        "backgroundColor": "#0a0a0a"
      },
      "splash": {
        "image": "./src/assets/icons/insta-logo-rounded.png",
        "resizeMode": "cover",
        "backgroundColor": "#0a0a0a",
        "dark": {
          "image": "./src/assets/icons/insta-logo-rounded.png",
          "resizeMode": "cover",
          "backgroundColor": "#0a0a0a"
        }
      },
      "package": "com.utkarshh.instagram"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/icons/insta-logo-rounded.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a0a0a"
        }
      ],
      "expo-asset",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
